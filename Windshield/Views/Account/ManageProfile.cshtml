@model Windshield.ViewModels.ManageProfileViewModel



<div class="row-fluid">
	<h2>Manage Profile</h2>
</div>

<div class="row-fluid">
	<div class="span6">
		@Html.Partial("_UserProfilePartial", Model)
	</div>
	<div class="span6">
		<div>
			@Html.Partial("_ChangePasswordPartial", Model.changePasswordModel)
		</div>
		<div>
			@Html.Partial("_UploadPhotoPartial")
		</div>
	</div>
</div>




@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}