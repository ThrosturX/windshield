@model List<Windshield.Models.Board>
@{
	ViewBag.Title = "Games available";
}
	
	<table class="table table-striped">

		<thead>
			<tr>
				<th>
					Data 1
				</th>
				<th>
					data 2
				</th>
			</tr>
		</thead>

		<tbody>
			@foreach (var board in Model)
			{
			<tr>
				<td>
					@board.id
				</td>
				<td>
					@board.Game.name
				</td>
			</tr>
			}	
		</tbody>

	</table>

