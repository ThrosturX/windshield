@model Windshield.ViewModels.AccountChangeUserDetailsViewModel

@{
    ViewBag.Title = "Change Password";
}

<div class="row-fluid">
	<h2>Manage Profile</h2>
</div>

<div class="row-fluid">
	<div class="span6">
		@Html.Partial("_UserProfilePartial")
	</div>
	<div class="span6">
		<div class="row">
			@Html.Partial("_ChangePasswordPartial", Model.changePasswordModel)
		</div>
		<div class="row">
			@Html.Partial("_UploadPhotoPartial")
		</div>
	</div>
</div>




@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}